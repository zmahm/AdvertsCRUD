{% extends 'base.html.twig' %}

{% block body %}
    <h1>Register</h1>
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.name) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}
    <button type="submit">Register</button>
    {{ form_end(form) }}
{% endblock %}
